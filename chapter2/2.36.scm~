#lang planet neil/sicp

(define (accumulate op initial sequence)
  (if (null? sequence)
      inital
      (op (car sequence)
          (accumulate op initial (cdr sequence)))))

(define (accumulate-n op init seqs)
  (if (null? (car seqs))
      nil
      (cons (accumulate op init (car seqs))
            (accumulate-n op init (cdr seqs)))))